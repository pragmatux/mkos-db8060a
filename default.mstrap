[general]
arch=armel
noauth=true
unpack=true
cleanup=true
omitrequired=true
markauto=true
bootstrap=ptux emdebian debian
aptsources=
aptpreferences=/usr/share/mkos-db8060a/preferences

[ptux]
source=http://packages.pragmatux.org/device
suite=bravo
components=main upstream
packages={{package}}

[emdebian]
source=http://emdebian.org/grip
suite=wheezy-grip

[debian]
source=http://cdn.debian.net/debian
suite=wheezy
